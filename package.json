{
  "name": "vuejs-vuex-webpack-seed",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "Emiliano Errecalde <eerrecalde@gmail.com>",
  "private": true,
  "engines": {
    "node": "6"
  },
  "scripts": {
    "start": "node ./server/index.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "VUE_ENV=client NODE_ENV=production webpack --config ./config/webpack.client.prod.js --progress --hide-modules && cp -R ./static ./dist",
    "build:server": "VUE_ENV=server NODE_ENV=production webpack --config ./config/webpack.server.js --progress --hide-modules",
    "test": "npm run test:lint && npm run test:unit && npm run test:e2e -- --env phantomjs",
    "test:unit": "node ./node_modules/karma-cli/bin/karma start ./config/karma.unit.js",
    "test:unit:dev": "npm run test:unit -- --no-single-run",
    "test:e2e": "node ./script/runner.e2e.js",
    "test:lint": "npm run test:lint:eslint && npm run test:lint:stylelint",
    "test:lint:eslint": "eslint --ignore-path .gitignore --ext .js,.vue .",
    "test:lint:stylelint": "stylelint 'src/**/*.@(?(s)?(a|c)ss|vue|html)' --config ./.stylelintrc.js"
  },
  "dependencies": {
    "alertify-js": "0.3.12",
    "babel-runtime": "6.18.0",
    "vue": "2.1.3",
    "vue-router": "2.0.3",
    "vuex": "2.0.0",
    "vuex-router-sync": "3.0.0"
  },
